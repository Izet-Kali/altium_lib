Ech11 141 3 VALUE = { IF (V(1555)<-13,2.22222222222222E-02*(V(1555)--13)+6,0) }
S112 136 146 1146 3 VON
Ech112 1146 3 VALUE = { IF ((V(1555)>=-13) & (V(1555)<=13),5,0) }
Ech12 146 3 VALUE = { IF ((V(1555)>=-13) & (V(1555)<=13),((6-4.3)/((-13-0.630511955306891)*(-13-0.630511955306891)))*(V(1555)-0.630511955306891)*(V(1555)-0.630511955306891) + 4.3,0) }
S113 136 144 1144 3 VON
Ech113 1144 3 VALUE = { IF (V(1555)>13,5,0) }
Ech13 144 3 VALUE = { IF (V(1555)>13,-1.11111111111112E-02*(V(1555)-13)+5.7,0) }
RIN1	136 3	1G
EOUT1 137 181	POLY(2) (136,3) (180,3) 0 0 0 0 0.999/1000
FCOPY1	3 180 VSENSE1 1
RSENSOR1 180 3	1K
VSENSE1 181 184	0
*
* TON/ TOFF/ BBM
S11 182 184 140 3 VON
S12 143 138 143 3 VEN
Ech14 143 3 VALUE = { IF(V(6)>=2.0, 5 , 0.8 ) }
eV1 140 3 138 3 1
C16 138 3 1e-012
*
* VOLTAGE SUPPLY REQUIREMENT
S13 1 182 185 3 VON
S14 142 185 142 3 VON
Ech15 142 3 VALUE = { IF((V(7)<=-0.5 & V(7)>=-22) & (V(4)<=22 & V(4)>=9), 5 , 0.01 ) }
S15 139 185 139 3 VON
Ech16 139 3 VALUE = { IF((V(7)>=-0.5 & (V(4)<=40 & V(4)>=9)), 5 , 0.01 ) }

.ENDS ADG5401
